{
  "name": "resdeeds",
  "version": "0.1.0",
  "description": "ResDEEDS - PyPSA Network Designer",
  "main": "./out/main/index.js",
  "author": "INL",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "start": "electron-vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "build:backend": "cd src/backend && uv run build.py",
    "build:backend:clean": "cd src/backend && uv run build.py --no-deps",
    "verify:backend": "cd src/backend && uv run build.py --verify-only",
    "dist": "npm run build && npm run build:backend && electron-builder",
    "dist:mac": "npm run build && npm run build:backend && electron-builder --mac",
    "dist:win": "npm run build && npm run build:backend && electron-builder --win",
    "dist:linux": "npm run build && npm run build:backend && electron-builder --linux"
  },
  "keywords": [
    "pypsa",
    "power-system",
    "network-designer"
  ],
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@eslint/js": "^9.32.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "electron": "^37.2.6",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "happy-dom": "^18.0.1",
    "memfs": "^4.36.0",
    "sass": "^1.90.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@xyflow/react": "^12.8.2",
    "bootstrap": "5.3.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.7.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron"
    ]
  }
}